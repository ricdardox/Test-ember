<section>
	
  <div class="container">
		{{outlet}}
	</div>


  <div class="container">
    <div class="page-header">
        <h1 id="timeline">Timeline</h1>
    </div>
    <ul class="timeline">
   
    {{#each}}
          <li id={{id}}>
          <div class="timeline-badge"><span class="glyphicon glyphicon-record"></span></div>
          <div class="timeline-panel">
            <div class="timeline-heading">
              <h4 class="timeline-title">
                {{titulo}}
              </h4>
              <p>
                <small class="text-muted">
                  <i class="glyphicon glyphicon-time"></i> {{fecha}}
                </small>
              </p>
            </div>
            <div class="timeline-body">
              {{#view "oembed"}}
                  <button class="visualizar btn btn-primary btn-xs">
                    Visualizar
                  </button>
              {{/view}}
              <p class="cont">{{contenido}}</p> 
              <p class="cont1"></p> 
              <p>{{autor}}</p>
            </div>
            <hr> 
          </div>
        </li>    
{{/each}}
    
    </ul>
</div>

</section>
 

 
